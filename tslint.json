{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-classes-per-file": 7,
        "max-file-line-count": [true, 500],
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "max-line-length": [true, 150],
        "no-console": [true, "log", "error"]
    },
    "rulesDirectory": []
}
